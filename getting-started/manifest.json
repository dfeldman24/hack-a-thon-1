{
    "name": "Pupperize",
    "description": "Replace page images with puppy pictures",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
      "service_worker": "background.js"
    },
    "permissions": ["storage", "activeTab", "scripting"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "/images/mike.png",
        "32": "/images/mike.png",
        "48": "/images/mike.png",
        "128": "/images/mike.png"
      }
    },
    "icons": {
      "16": "/images/mike.png",
      "32": "/images/mike.png",
      "48": "/images/mike.png",
      "128": "/images/mike.png"
    }
  }